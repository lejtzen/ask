{
  "name": "my-site",
  "private": true,
  "scripts": {
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "gridsome": "^0.7.21"
  },
  "devDependencies": {
    "axios": "^0.21.0",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "overrides": [
      {
        "files": [
          "*.html",
          "*.js",
          "*.vue",
          "*.css",
          "*.scss"
        ],
        "options": {
          "tabWidth": 4
        }
      }
    ]
  }
}
